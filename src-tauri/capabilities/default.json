{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "core:window:default",
    "core:window:allow-start-dragging",
    "core:event:allow-listen",
    "allow-get-scap-targets",
    "allow-start-scap-capture",
    "allow-stop-scap-capture"
  ]
}
